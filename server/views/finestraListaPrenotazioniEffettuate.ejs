<!DOCTYPE html>
<html lang="it">
  <head>
    <% include partials/head.ejs %>
  </head>
  <body style="padding-top: 72px;">
    <% include partials/header.ejs %>
    <section class="py-5">
      <div class="container">
        <!-- Breadcrumbs -->
        <ol class="breadcrumb pl-0  justify-content-start">
          <li class="breadcrumb-item"><a href="/index">Home</a></li>
          <li class="breadcrumb-item active">I tuoi viaggi   </li>
        </ol>
        
        <div class="row">
          <h1 class="col-12 hero-heading mb-4">Prossimi viaggi</h1>
          <!-- place item-->
          <% data.dataC.forEach(element => { %>
          <div class="col-xl-3 col-md-4 col-sm-6 mb-5" data-marker-id="59c0c8e33b1527bfe2abaf92">
            <div class="card h-100 border-0 shadow">
              <div class="card-img-top overflow-hidden"><a href="/profilo/finestraPrenotazioneEffettuata"><img class="img-fluid" src="https://d19m59y37dris4.cloudfront.net/directory/1-5/img/photo/photo-1484154218962-a197022b5858.jpg" alt="Modern, Well-Appointed Room"/></a></div>
              <div class="card-body d-flex align-items-center">
                <div class="w-100">
                  <p class="subtitle font-weight-normal text-sm mb-2"><%= element.data_inizio %></p>
                  <h6 class="card-title"><a class="text-decoration-none text-dark" href="/profilo/finestraPrenotazioneEffettuata"><%= element.alloggio %></a></h6>
                  <div class="d-flex card-subtitle mb-3">
                    <p class="flex-grow-1 mb-0 text-muted text-sm">Private room</p>
                    <p class="flex-shrink-1 mb-0 card-stars text-xs text-right"><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i>
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% }); %>
        </div>
        <div class="row">
          <h2 class="col-12 hero-heading mb-4">Viaggi passati</h2>
          <!-- place item-->
          <% data.dataNC.forEach(element => { %>
            <div class="col-xl-3 col-md-4 col-sm-6 mb-5" data-marker-id="59c0c8e33b1527bfe2abaf92">
              <div class="card h-100 border-0 shadow">
                <div class="card-img-top overflow-hidden"><a href="/profilo/finestraPrenotazioneEffettuata"><img class="img-fluid" src="https://d19m59y37dris4.cloudfront.net/directory/1-5/img/photo/photo-1484154218962-a197022b5858.jpg" alt="Modern, Well-Appointed Room"/></a></div>
                <div class="card-body d-flex align-items-center">
                  <div class="w-100">
                    <p class="subtitle font-weight-normal text-sm mb-2"><%= element.data_inizio %></p>
                    <h6 class="card-title"><a class="text-decoration-none text-dark" href="/profilo/finestraPrenotazioneEffettuata"><%= element.alloggio %></a></h6>
                    <div class="d-flex card-subtitle mb-3">
                      <p class="flex-grow-1 mb-0 text-muted text-sm">Private room</p>
                      <p class="flex-shrink-1 mb-0 card-stars text-xs text-right"><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i><i class="fa fa-star text-warning"></i>
                      </p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <% }); %>
        </div>
      </div>
    </section>
    <!-- Footer-->
    <% include partials/footer.ejs %>
    <!-- JavaScript files-->
    <% include partials/scriptFooter.ejs %>

    <!-- jQuery>
    <script>
      $(document).ready(function() {

      })
    </script -->
  </body>
</html>