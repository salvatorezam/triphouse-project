<script>
    // ------------------------------------------------------- //
    //   Normalmente dovremmo includere lo sprite in testa al documento, ma questo significherebbe avere una replica degli svg in ogni pagina,
    //   inoltre il parser HTML dovrebbe prima ispezionare questi file nella loro interezza prima di poter proseguire.
    //   La questione si risolve tramite una apposita richiesta ajax, che permette conseguentemente di sfruttare la cache del browser
    //   per reperire i suddetti svg. Bisogna quindi assicurarsi che l'svg Ã¨ nel namespace corretto prima che venga fatto l'append alla pagina:
    //   questo lo si fa prima appendendo l'svg ad un div, e poi appendendo il risultato alla pagina, pertanto: 
    function injectSvgSprite(path) {
    
        var ajax = new XMLHttpRequest();
        ajax.open("GET", path, true);
        ajax.send();
        ajax.onload = function(e) {
        var div = document.createElement("div");
        div.className = 'd-none';
        div.innerHTML = ajax.responseText;
        document.body.insertBefore(div, document.body.childNodes[0]);
        }
    }    
    injectSvgSprite('https://demo.bootstrapious.com/directory/1-0/icons/orion-svg-sprite.svg'); 
  </script> 

